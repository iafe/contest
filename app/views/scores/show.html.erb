<div class="container">

<p id="notice"><%= notice %></p>

<p>
  <strong>Judge:</strong>
  <%= @score.user.first_name %> <%= @score.user.last_name %>
</p>

<p>
  <strong>Category:</strong>
  <%= @score.submission.category.award.name %> <%= @score.submission.category.code %>: <%= @score.submission.category.name %>
</p>

<p>
  <strong>Organization:</strong>
  <%= @score.submission.organization.name %>
</p>

<p>
  <strong>Division:</strong>
  <%= @score.submission.division.name %>
</p>

<p>
  <strong>Total score:</strong>
  <%= @score.total_score %>
</p>

<p>
  <strong>Comments:</strong>
  <%= @score.comments %>
</p>

<p>
  <strong>Disqualify:</strong>
  <%= @score.disqualify %>
</p>

<p><%= link_to 'Return to Your Judging Dashboard', scores_path %></p>

</div>
